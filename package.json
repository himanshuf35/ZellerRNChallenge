{
  "name": "ZellerRnChallenge",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --variant='devDebug' && npm run checkFlipper",
    "androidQA": "react-native run-android --variant='qaDebug' && npm run checkFlipper",
    "androidUAT": "react-native run-android --variant='uatDebug' && npm run checkFlipper",
    "androidProd": "react-native run-android --variant='prodDebug' && npm run checkFlipper",
    "ios": "react-native run-ios --scheme 'DEV' --simulator 'iPhone 14' && npm run checkFlipper",
    "iosQA": "react-native run-ios --scheme 'QA' --simulator 'iPhone 14' && npm run checkFlipper",
    "iosUAT": "react-native run-ios --scheme 'UAT' --simulator 'iPhone 14' && npm run checkFlipper",
    "iosProd": "react-native run-ios --scheme 'PROD' --simulator 'iPhone 14' && npm run checkFlipper",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "checkFlipper": "sh './src/scripts/flipper.sh'",
    "podInstall": "cd ios && pod install && cd ..",
    "postinstall": "cd graphQLServer/mock-server && npm install",
    "startGraphQLServer": "cd graphQLServer/mock-server && npm start",
    "iOSApp": "concurrently 'npm run ios' 'npm run startGraphQLServer'",
    "androidApp": "concurrently 'npm run android' 'npm run startGraphQLServer'"
  },
  "dependencies": {
    "@apollo/client": "^3.7.17",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-community/netinfo": "9.3.7",
    "@react-native-masked-view/masked-view": "0.2.8",
    "@react-navigation/bottom-tabs": "6.5.7",
    "@react-navigation/drawer": "6.6.2",
    "@react-navigation/native": "6.1.6",
    "@react-navigation/native-stack": "6.9.12",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "1.3.4",
    "graphql": "^16.7.1",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-native": "0.71.4",
    "react-native-config": "1.5.0",
    "react-native-device-info": "10.4.0",
    "react-native-flipper": "0.184.0",
    "react-native-gesture-handler": "2.9.0",
    "react-native-reanimated": "3.0.2",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "3.20.0",
    "react-native-svg": "13.8.0",
    "react-redux": "8.0.5",
    "redux": "4.2.1",
    "redux-flipper": "2.0.2",
    "redux-persist": "6.0.0",
    "rn-async-storage-flipper": "0.0.10"
  },
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@babel/preset-env": "7.20.2",
    "@babel/runtime": "7.21.0",
    "@react-native-community/eslint-config": "3.2.0",
    "@tsconfig/react-native": "2.0.3",
    "@types/jest": "29.4.1",
    "@types/react": "18.0.28",
    "@types/react-test-renderer": "18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "babel-jest": "29.5.0",
    "concurrently": "^8.2.0",
    "eslint": "8.36.0",
    "jest": "29.5.0",
    "metro-react-native-babel-preset": "0.76.0",
    "prettier": "2.8.4",
    "react-native-svg-transformer": "1.0.0",
    "react-test-renderer": "18.2.0",
    "sonarqube-scanner": "3.0.1",
    "typescript": "^5.1.6"
  },
  "jest": {
    "preset": "react-native"
  }
}
